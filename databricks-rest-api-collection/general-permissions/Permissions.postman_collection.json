	"item": [
		{
			"name": "01-get workspace object id (directories and notebooks)",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "64216dae-73c6-4d44-8f1d-f2f6e43ee626",
						"exec": [
							"pm.test(pm.info.requestName, () => {",
							"    pm.response.to.not.be.error;",
							"    pm.response.to.not.have.jsonBody('error');",
							"});",
							"",
							"let body = JSON.parse(responseBody),",
							"    object_ids = _.map(body.objects, (object) => object.object_id);",
							"",
							"",
							"// Store all the object ids in the environment variable",
							"pm.globals.set('object_ids', JSON.stringify(object_ids));",
							"",
							"// Store the next index of the array for object ids",
							"//pm.environment.set('nextIndex', 0);",
							"",
							"// Store the active PR ID to fetch the comments for",
							"pm.globals.set('notebook_id', object_ids[0]);",
							"pm.globals.set('directories_id', object_ids[1]);",
							"",
							"",
							"//(pm.response.json().objects[0].object_type == \"DIRECTORY\")",
							"//pm.globals.set(\"directories_id\", pm.response.json().objects[0].object_type);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{db_host}}/api/2.0/workspace/list?path=/Users/bhavin.kukadia@databricks.com/",
					"host": [
						"{{db_host}}"
					],
					"path": [
						"api",
						"2.0",
						"workspace",
						"list"
					],
					"query": [
						{
							"key": "path",
							"value": "/Users/bhavin.kukadia@databricks.com/"
						}
					]
				}
			},
			"response": []
		},
